USER METHOD:
	mkdir DIR			--create the directory
	vim FILE			--create a file named FILE
	mv FILE1 FILE2		--move the FILE1 to FILE2
	cp FILE1 FILE2		--copy the FILE1 to FILE2
	rm FILE				--remove a file named FILE
	get FILE			--get the FILE from server
	put FILE			--put the FILE into server
	find FILE			--find the FILE from client
	cat FILE			--scan the FILE online
	class FILE			--
